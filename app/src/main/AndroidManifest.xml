<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="thedorkknightrises.asdtest">

    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        tools:ignore="GoogleAppIndexingWarning">
        <activity android:name=".ui.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".ui.ASDTestActivity"
            android:label="@string/questionnaire" />
        <activity
            android:name=".ui.PersonalDetailsActivity"
            android:label="@string/personal_details" />
        <activity
            android:name=".ui.ConfigurationActivity"
            android:label="@string/configuration" />
        <activity
            android:name=".ui.AutismTestActivity"
            android:label="@string/questionnaire" />
    </application>

</manifest>